{
  "name": "@0x84d0u/portfolio-ui",
  "version": "0.1.0",
  "private": false,
  "description": "A personal UI library for portfolio projects, built with Shadcn/UI and Tailwind CSS.",
  "type": "module",
  "main": "./dist/portfolio-ui.umd.cjs",
  "module": "./dist/portfolio-ui.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
