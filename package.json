{
  "name": "@0x84d0u/portfolio-ui",
  "version": "0.1.2",
  "private": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0x84d0u/portfolio-ui.git"
  },
  "bugs": {
    "url": "https://github.com/0x84d0u/portfolio-ui/issues"
  },
  "homepage": "https://github.com/0x84d0u/portfolio-ui#readme",
  "description": "A modern React UI component library.",
  "author": "Naoufal Badou contact@naoufalbadou.com>",
  "keywords": [
    "react",
    "typescript",
    "components",
    "ui",
    "tailwindcss",
    "shadcn"
  ],
  "license": "MIT",
  "exports": {
    "./globals.css": "./dist/globals.css",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./helpers": {
      "import": {
        "types": "./dist/helpers.d.ts",
        "default": "./dist/helpers.js"
      },
      "require": {
        "types": "./dist/helpers.d.ts",
        "default": "./dist/helpers.cjs"
      }
    },
    "./patterns": {
      "import": {
        "types": "./dist/patterns.d.ts",
        "default": "./dist/patterns.js"
      },
      "require": {
        "types": "./dist/patterns.d.ts",
        "default": "./dist/patterns.cjs"
      }
    },
    "./ui": {
      "import": {
        "types": "./dist/ui.d.ts",
        "default": "./dist/ui.js"
      },
      "require": {
        "types": "./dist/ui.d.ts",
        "default": "./dist/ui.cjs"
      }
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "release": "npm version patch && npm run build && npm publish"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/vite": "^4.1.13",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "postcss": "^8.5.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lib-inject-css": "^2.2.2"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "lucide-react": "^0.544.0",
    "next-themes": "^0.4.6",
    "sonner": "^2.0.7"
  }
}
